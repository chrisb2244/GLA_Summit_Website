{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mdi/js": "^7.0.96",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@next/font": "^13.0.1-canary.2",
    "@supabase/auth-helpers-nextjs": "^0.4.0-next.3",
    "@supabase/auth-helpers-react": "^0.3.0-next.2",
    "@supabase/supabase-js": "^2.0.0-rc.10",
    "form-data": "^4.0.0",
    "formik": "^2.2.9",
    "mailgun.js": "^7.0.0",
    "next": "^13.0.1-canary.2",
    "nodemailer": "^6.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^8.0.0-alpha.4",
    "swr": "^1.3.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "dev-turbo": "next dev --turbo",
    "dev-turbo-wcss": "concurrently \"next dev --turbo\" \"postcss ./src/**/*.css --dir ./src/css --watch\"",
    "dev-debug": "concurrently \"next dev\" \"free -s 5 -h\"",
    "dev-turbo-debug": "concurrently \"next dev --turbo\" \"free -s 5 -h\"",
    "test": "jest && npx playwright test",
    "test-w": "jest --watch",
    "playwright": "playwright test",
    "lint": "eslint --format visualstudio ./src/ ./__tests__/ ./playwright/",
    "lint-f": "eslint --format visualstudio --fix ./src/ ./__tests__/",
    "precommit-tests": "LIST=$(git diff-index --name-only --diff-filter=d HEAD | grep -E \"(.js$|.jsx$|.ts$|.tsx$)\" | sed -e 's|frontend/||g'); if [ \"$LIST\" ]; then eslint ${LIST} ${LIST}; else echo 'No files to lint'; fi && npx jest && npx playwright test",
    "husky-install": "cd .. && husky install frontend/.husky && cd frontend"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.0.0",
    "@playwright/test": "^1.27.1",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.18.2",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.12.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-react": "^0.1.0",
    "eslint-plugin-testing-library": "^5.2.1",
    "husky": "^7.0.0",
    "is-ci": "^3.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "msw": "^0.47.4",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
