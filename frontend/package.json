{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@next-auth/sequelize-adapter": "^1.0.2",
    "formik": "^2.2.9",
    "jest": "^27.5.1",
    "mysql2": "^2.3.3",
    "next": "^12.1.0",
    "next-auth": "^4.3.1",
    "nodemailer": "^6.7.3",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "sequelize": "^6.17.0",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "test": "jest",
    "test-w": "jest --watch",
    "lint": "eslint --format visualstudio ./src/ ./__tests__/",
    "lint-f": "eslint --format visualstudio --fix ./src/ ./__tests__/",
    "precommit-tests": "LIST=$(git diff-index --name-only --diff-filter=d HEAD | grep -E \"(.js$|.jsx$|.ts$|.tsx$)\" | sed -e 's|frontend/||g'); if [ \"$LIST\" ]; then eslint ${LIST} ${LIST}; else echo 'No files to lint'; fi && npx jest",
    "prepare": "is-ci || cd .. && husky install frontend/.husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.15",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cypress": "^9.5.1",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-react": "^0.1.0",
    "husky": "^7.0.0",
    "is-ci": "^3.0.1",
    "ts-node": "^10.7.0"
  }
}
